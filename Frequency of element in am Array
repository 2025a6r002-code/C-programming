#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d", &n);
    
    int b[n];
    for(int i=0; i<n ; i++){
        scanf("%d", &b[i]);
    }
    
    int c, d=0;
    scanf("%d", &c);
    for(int i=0 ; i<n ; i++){
        if (b[i]==c){
            d++;
        }
    }
    printf("%d", d);
    return 0;
}
