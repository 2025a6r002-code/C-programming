* * * *
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int N , M;
    scanf("%d %d",&N , &M);
    for(int i=1; i<=N; i++){
        for(int j=1; j<=M ;j++){
            if( j==1 || j==M || i==1 || i==N){
           printf("* ");
            }
            else{
               printf("  ");
            }
        }
        printf("\n");
    }
    return 0;
}
