\\WAP to convert total numbers of days into years, weeks and days.

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int days,weeks,years;
    scanf("%d",&days);
    years=days/365;
    days=days%365;
    weeks=days/7;
    days=days%7;
        printf("Years: %d\n",years);
        printf("Weeks: %d\n",weeks);
        printf("Days: %d",days);
     
    return 0;
}
