#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d", &n);
    int a[n];
    for(int i=0; i< n ; i++){
        scanf("%d",&a[i]);
    }
    
    int e , f=0;
    scanf("%d", &e);
    for(int i=0; i< n ; i++){
        if(a[i]==e){
            printf("Element found at position %d",i+1);
            f=1;
            break;
        }
    }
    
    if(f != 1){
        printf("Element not found");
    }
    return 0;
}
