//WAP to find maximum out of n numbers.

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n, i;
    int num, max;
    scanf("%d", &n);
    scanf("%d", &num);
    max=num;
     for(i=2; i<=n ; i++){
    scanf("%d", &num);
     if (num>max)
         max=num;
     }
    printf("%d",max);
    
    return 0;
}
