\\WAP to swap two numbers.
\\ without third variable

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int x, y;
    scanf("%i%i",&x ,&y);
    x=x+y;
    y=x-y;
    x=x-y;
    printf("%i %i",x,y);
    return 0;
}


\\with third variable
#include<stdio.h>
int main(){
      int x,y,z;
      scanf("%d%d",&x,&y);
      z=x;
      x=y;
      y=z;
      printf("%d %d",x ,y);
      return 0;
}
      
