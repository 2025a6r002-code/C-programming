#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int N,a,b,n;
    scanf("%d",&N);
    if(N>=0 && N<=9){
        printf("%d",N);
    }
    else{
        int temp= N;
        while (temp != 0){
            temp= temp/10;
            n++;
        };
    a=N/(int)pow(10,n-1);
    N=N%(int)pow(10,n-1);
    b=N%10;
    N=N/10;
    N=b*pow(10,n-1) + N*10 + a;
    printf("%d",N);
    }
  return 0;
}

